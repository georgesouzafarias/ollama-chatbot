{
    "name": "js-ollama-chat",
    "version": "1.0.0",
    "description": "Interactive chat application using Ollama",
    "type": "module",
    "main": "dist/app.js",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/app.js",
        "dev": "ts-node src/app.ts",
        "dev:watch": "ts-node --watch src/app.ts",
        "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "ollama",
        "chat",
        "ai",
        "cli"
    ],
    "author": "George Farias",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/claude-code": "^1.0.44",
        "ollama": "^0.5.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/jest": "^30.0.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    }
}
