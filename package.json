{
    "name": "js-ollama-chat",
    "version": "1.0.0",
    "description": "Interactive chat application using Ollama",
    "type": "module",
    "main": "src/app.js",
    "scripts": {
        "start": "node src/app.js",
        "dev": "node --watch src/app.js",
        "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
        "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
        "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
        "test:config": "NODE_OPTIONS='--experimental-vm-modules' jest tests/config/constants.test.js",
        "test:prompts": "NODE_OPTIONS='--experimental-vm-modules' jest tests/prompts/system-prompt.test.js"
    },
    "keywords": [
        "ollama",
        "chat",
        "ai",
        "cli"
    ],
    "author": "George Farias",
    "license": "MIT",
    "dependencies": {
        "ollama": "^0.5.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "jest": "^29.7.0"
    }
}